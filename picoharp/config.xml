<taurusgui_config>
  <GUI_NAME>PicoHarp</GUI_NAME>
  <ORGANIZATION>ALBA</ORGANIZATION>
  <CUSTOM_LOGO/>
  <PanelDescriptions>
    <PanelDescription>
      <name>Histogram</name>
      <classname>HistogramPlot</classname>
      <modulename>widgets.panels</modulename>
      <widgetname/>
      <floating>True</floating>
      <sharedDataWrite/>
      <sharedDataRead/>
      <model>bl34/di/PhCt-01/histogram</model>
      <!--model>bl34/di/PhCt-01</model-->
    </PanelDescription>
    <PanelDescription>
      <name>Channel 0</name>
      <classname>Channel0Form</classname>
      <modulename>widgets.panels</modulename>
      <widgetname/>
      <floating>True</floating>
      <sharedDataWrite/>
      <sharedDataRead/>
      <model>bl34/di/PhCt-01/zerocrossch0, 
             bl34/di/PhCt-01/levelch0, 
             bl34/di/PhCt-01/syncdivider</model>
    </PanelDescription>
    <PanelDescription>
      <name>Channel 1</name>
      <classname>Channel1Form</classname>
      <modulename>widgets.panels</modulename>
      <widgetname/>
      <floating>True</floating>
      <sharedDataWrite/>
      <sharedDataRead/>
      <model>bl34/di/PhCt-01/zerocrossch1, 
             bl34/di/PhCt-01/levelch1</model>
    </PanelDescription>
    <PanelDescription>
      <name>Acquisition</name>
      <classname>AcquisitionForm</classname>
      <modulename>widgets.panels</modulename>
      <widgetname/>
      <floating>True</floating>
      <sharedDataWrite/>
      <sharedDataRead/>
      <model>bl34/di/PhCt-01/resolution, 
             bl34/di/PhCt-01/binning, 
             bl34/di/PhCt-01/offset, 
             bl34/di/PhCt-01/acquisitiontime, 
             bl34/di/PhCt-01/ElapsedMeasTime,
             bl34/di/PhCt-01/overflowstopper, 
             bl34/di/PhCt-01/overflowstopperthreshold</model>
    </PanelDescription>
    <PanelDescription>
      <name>Monitor</name>
      <classname>MonitorForm</classname>
      <modulename>widgets.panels</modulename>
      <widgetname/>
      <floating>True</floating>
      <sharedDataWrite/>
      <sharedDataRead/>
      <model>bl34/di/PhCt-01/countratech0, 
             bl34/di/PhCt-01/countratech1, 
             bl34/di/PhCt-01/integralcount, 
             bl34/di/PhCt-01/HistogramMaxValue</model>
    </PanelDescription>
    <PanelDescription>
      <name>Commands</name>
      <classname>PhCtCommands</classname>
      <modulename>widgets.panels</modulename>
      <widgetname/>
      <floating>False</floating>
      <sharedDataWrite/>
      <sharedDataRead/>
      <model>bl34/di/PhCt-01</model>
    </PanelDescription>
    <PanelDescription>
      <name>Status</name>
      <classname>StateForm</classname>
      <modulename>widgets.panels</modulename>
      <widgetname/>
      <floating>True</floating>
      <sharedDataWrite/>
      <sharedDataRead/>
      <model>bl34/di/PhCt-01/state, 
             bl34/di/PhCt-01/status</model>
    </PanelDescription>
    <PanelDescription>
      <name>Warnings</name>
      <classname>WargningForm</classname>
      <modulename>widgets.panels</modulename>
      <widgetname/>
      <floating>True</floating>
      <sharedDataWrite/>
      <sharedDataRead/>
      <model>bl34/di/PhCt-01/Warnings</model>
    </PanelDescription>
  </PanelDescriptions>
  <MONITOR/>
</taurusgui_config>
